{
  "name": "BloodDonor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --forceExit",
    "lint": "eslint index.js src",
    "start:nocache": "npm run start -- --reset-cache",
    "test:debug": "jest --runInBand --detectOpenHandles",
    "test:watchall": "npm run test:debug -- --watchAll",
    "test:watch": "npm run test:debug -- --watchAll",
    "clean:env": "rm -rf node_modules/.cache/babel-loader/*",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "clean:all": "npm run clean:env && watchman watch-del-all && rm -rf /tmp/metro-* && npm run clean:android && npm run clean:ios"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.1",
    "@react-navigation/native": "^5.8.9",
    "@react-navigation/stack": "^5.12.6",
    "appcenter": "4.0.0",
    "appcenter-analytics": "4.0.0",
    "appcenter-crashes": "4.0.0",
    "axios": "^0.21.0",
    "axios-mock-adapter": "^1.19.0",
    "jetifier": "^1.6.6",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-dotenv": "^2.4.2",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-onboarding-swiper": "^1.1.4",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.0",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "expect": "^26.6.2",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.0",
    "react-dom": "^17.0.1",
    "react-test-renderer": "16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/**/*.js --fix-dry-run"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
